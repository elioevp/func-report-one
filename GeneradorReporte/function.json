{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "documents",
      "databaseName": "%CosmosDbDatabaseName%",
      "collectionName": "%CosmosDbContainerName%",
      "connectionStringSetting": "CosmosDbConnectionString",
      "sqlQuery": "SELECT * FROM c WHERE c.username = {username} AND c.directorio = {directorio}"
    }
  ]
}